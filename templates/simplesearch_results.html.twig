{% extends 'partials/simplesearch_base.html.twig' %}

{% block content %}

<div class="wrapper">
<div class="container" id="main">
    <section>
      <h2>{{'TELEPHASIC.BLOG.SEARCH.RESULTS'|t}}</h2>
      {% include 'partials/simplesearch_searchbox.html.twig' %}
    </section>
    <section>
      {% for page in search_results %}
        {% include 'partials/simplesearch_item.html.twig' with { 'page':page, 'page_title': page.title } %}
      {% endfor %}
    </section>
</div>
</div>
 
{% endblock %}

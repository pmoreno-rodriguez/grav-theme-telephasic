{% extends 'partials/base.html.twig' %}

{% block content %}

<div class="page-wrapper">
<div class="wrapper">
<div class="container" id="main">
      <header>
          <h2>{{page_title|raw}}</h2>
          {% if page_subtitle %}<p>{{ page_subtitle|raw }}</p>{% endif %}
          {{ primaryImage.loading('lazy').attribute('decoding', 'async').attribute('itemprop', 'image').html(page_title|raw, page_title|raw, 'image featured')|raw }}
      </header>
      <div>
        {{ page.content|raw }}
      </div>
</div>
</div>
</div>

{% endblock %}
